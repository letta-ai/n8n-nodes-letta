{
  "name": "@letta/n8n-nodes-letta",
  "version": "0.1.0",
  "description": "Official n8n node for integrating with Letta AI agent platform",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "letta",
    "ai",
    "agents"
  ],
  "license": "MIT",
  "homepage": "https://github.com/letta-ai/letta-n8n-node",
  "author": {
    "name": "Letta",
    "email": "contact@letta.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/letta-ai/letta-n8n-node.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "prepack": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/LettaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Letta/Letta.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {}
}
