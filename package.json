{
  "name": "@letta-ai/n8n-nodes-letta",
  "version": "0.0.5",
  "description": "Official n8n node for integrating with Letta AI agent platform",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "letta",
    "ai",
    "agents"
  ],
  "license": "MIT",
  "homepage": "https://github.com/letta-ai/n8n-nodes-letta",
  "author": {
    "name": "Letta",
    "email": "contact@letta.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/letta-ai/n8n-nodes-letta.git"
  },
  "scripts": {
    "build": "tsc && npm run copy:assets",
    "copy:assets": "cp nodes/Letta/letta.svg dist/nodes/Letta/",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepack": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/LettaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Letta/Letta.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "n8n-core": "^1.114.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@letta-ai/letta-client": "^0.0.68664"
  }
}
